ASSISTANT_NAME=Commander
ASSISTANT_TRIGGER=Commander

# Brain LLM (single place to configure commander's main model/backend)
BRAIN_MODEL=claude-sonnet-4-5
BRAIN_BASE_URL=
BRAIN_AUTH_TOKEN=
BRAIN_API_KEY=
BRAIN_OAUTH_TOKEN=

# Matrix configuration for commander
MATRIX_HOMESERVER=
MATRIX_ACCESS_TOKEN=
MATRIX_USER_ID=
MATRIX_USERNAME=
MATRIX_PASSWORD=
MATRIX_DEVICE_NAME=commander
# Matrix room jid for Commander room (format: matrix:!roomId:server).
# When set, terminal chat uses this as LOCAL_CHAT_JID so terminal and Matrix share the same context/state.
# Terminal inbound/outbound messages are also echoed into this room.
LOCAL_MIRROR_MATRIX_JID=
# Optional terminal sender label. Defaults from NanoClaw config if unset.
LOCAL_CHAT_SENDER_NAME=

# Container image
CONTAINER_IMAGE=nanoclaw-agent:latest
LOG_LEVEL=info
