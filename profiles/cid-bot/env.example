ASSISTANT_NAME=Cid

# Brain LLM (single place to configure cid-bot's main model/backend)
# Default intent: Codex-style MAIN via Claude Code harness.
# Set BRAIN_BASE_URL to your compatible endpoint for this model family.
BRAIN_MODEL=claude-sonnet-4-5
BRAIN_BASE_URL=
BRAIN_AUTH_TOKEN=
BRAIN_API_KEY=
BRAIN_OAUTH_TOKEN=
# Optional corporate/root CA bundle for in-container TLS trust.
# If unset, run.sh auto-uses ~/.ssh/corporate-certs.pem when present.
BRAIN_CA_CERT_FILE=

# Matrix configuration for cid-bot
MATRIX_HOMESERVER=
MATRIX_ACCESS_TOKEN=
MATRIX_USER_ID=
MATRIX_USERNAME=
MATRIX_PASSWORD=
MATRIX_DEVICE_NAME=cid-bot
CID_JOHNNY5_CHAT_JID=
# Matrix room jid for Cid Engineering room (format: matrix:!roomId:server).
# When set, terminal chat uses this as LOCAL_CHAT_JID so terminal and Matrix share the same context/state.
# Terminal inbound/outbound messages are also echoed into this room.
LOCAL_MIRROR_MATRIX_JID=
# Optional terminal sender label. Defaults from NanoClaw config if unset.
LOCAL_CHAT_SENDER_NAME=

# Container image
CONTAINER_IMAGE=nanoclaw-agent:latest
LOG_LEVEL=info
